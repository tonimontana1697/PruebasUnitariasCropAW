<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>*</from-view-id>
        <navigation-case>
            <from-outcome>Cultivo</from-outcome>
            <to-view-id>/Vistas/cultivo/List.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Personal</from-outcome>
            <to-view-id>/Vistas/persona/List.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Inicio</from-outcome>
            <to-view-id>/Vistas/index/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Sector</from-outcome>
            <to-view-id>/Vistas/Sector/List.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Informacion</from-outcome>
            <to-view-id>/Vistas/informacion/Informacion.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>DetalleInformacion</from-outcome>
            <to-view-id>/Vistas/informacion/Detalle_Informacion.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>AsignacionCultivo</from-outcome>
            <to-view-id>/Vistas/Asignacion/ListAsigCult.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>AsignacionPersonal</from-outcome>
            <to-view-id>/Vistas/Asignacion/ListAsig.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <application>
        <resource-bundle>
            <base-name>/Vistas</base-name>
            <var>vistas</var>
        </resource-bundle>
        <resource-bundle>
            <base-name>/Vistas</base-name>
            <var>vistas</var>
        </resource-bundle>
    </application>
</faces-config>
